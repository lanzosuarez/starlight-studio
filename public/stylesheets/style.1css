/*---------- Media Queries------------ */
@media screen and (max-width: 1023px) {

    #rocketLaptop{
      display: none;
    }
    #service-head, #service-sub{
      text-align: center
    }
    #pads{
      padding: 31px;
    }
}

@media screen and (max-width: 1022px) and (min-width: 64px){
    div#pads{
      padding: 31px;
    }
    div#offer{
      padding: 31px;
    }
    div#offer h2{
      padding: 54px 0 0px 0;
      text-align: center;
    }
    article#child{
      margin-bottom: 53px !important;
    }
}
@media screen and (max-width: 768px) and (min-width: 64px){
  .nav.rheader{
    height: 3em !important;
  }
  span.rlogo {
     font-size: 1.5em !important;
     left: 50px;
  }
  #rocketLogo{
    padding: 0.5em !important;
  }
  .nav-menu .nav-item {
    height: 40px;
  }
  #contactCov {
    padding: 0em 0 0em 0 !important;
  }
  .nav-toggle{
    right: 19px;
  }
}

