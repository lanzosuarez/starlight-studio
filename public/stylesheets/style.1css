	65% {
    bottom: 0%;
  }
  15%,
	40% {
    bottom: 248px;
  }
}
@-webkit-keyframes takeOff {
  0%,
	65% {
    bottom: 0%;
  }
  15%,
	40% {
    bottom: 248px;
  }
}
